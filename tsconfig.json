{
    "compilerOptions": {
        "lib": ["ESNext"],
        "target": "ES5",
        "strict": true,
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "outDir": "./dist",
        "rootDir": "./src",
        // "noUnusedLocals": true,
        "noUnusedParameters": true,
        "declaration": true,
        "declarationDir": "dist/types",
    }
}